CREATE TABLE weather_data (
id INT AUTO_INCREMENT PRIMARY KEY,
sido VARCHAR(50),
sigungu VARCHAR(50),
dong VARCHAR(50),
datetime DATETIME,
temperature FLOAT,
precipitation FLOAT,
snowfall FLOAT
);
